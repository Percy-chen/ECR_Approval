	<TableSelectDialog title="{/appProperties/f4title}" search="handleSearch" confirm="handleConfirm" cancel="handleClose"
		items="{/searchHelpData}">
		<items>
			<ColumnListItem type="Navigation" press="handleConfirm">
				<cells>
					<Text text="{ACCOUNT}"/>
					<Text text="{FULLNAME}"/>
					<Text text="{COMPANYCODE}"/>
					<Text text="{DEPARTMENT}"/>
				</cells>
			</ColumnListItem>
		</items>
		<columns>
			<Column>
				<Text text="賬號"/>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<Text text="姓名"/>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" hAlign="End">
				<Text text="公司"/>
			</Column>
			<Column hAlign="End">
				<Text text="部門"/>
			</Column>
		</columns>
	</TableSelectDialog>
